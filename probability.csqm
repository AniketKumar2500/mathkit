#if !defined(PROBABILITY_CSQM)
#define PROBABILITY_CSQM

    //importing basic.csqm
    #include "basic.csqm"
    /*This function calculates probability in terms of percentage.
     @param cand the argument for the number of items which are to be find.
     @param whole total number of items present.
    */
    double prob(double cand, double whole){
        return percent(cand, whole);
    }
    

#endif // PROBABILITY_CSQM